<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.river.learn.web.mapper.DataSourceMapper">

	<!-- 按条件查询列表 -->
	<select id="get" resultType="com.river.learn.web.bean.DataSourceBean">
		SELECT * FROM r_user
	</select>
	<insert id="put" parameterType="com.river.learn.web.bean.DataSourceBean">
		INSERT INTO r_user (id,name)
		VALUES (#{id},#{name})
	</insert>
</mapper>